/*
 * Solarized colorscheme:
 * $base03:    #002b36;
 * $base02:    #073642;
 * $base01:    #586e75;
 * $base00:    #657b83;
 * $base0:     #839496;
 * $base1:     #93a1a1;
 * $base2:     #eee8d5;
 * $base3:     #fdf6e3;
 * $yellow:    #b58900;
 * $orange:    #cb4b16;
 * $red:       #dc322f;
 * $magenta:   #d33682;
 * $violet:    #6c71c4;
 * $blue:      #268bd2;
 * $cyan:      #2aa198;
 * $green:     #859900;
 * */
digraph hierarchy 
{
  size="15,15"
    node[shape=record,style=filled,fillcolor="#fdf6e3", fontcolor="#657b83", color="#eee8d5"]
    edge[dir=forward, arrowtail=open, color="#586e75"]

